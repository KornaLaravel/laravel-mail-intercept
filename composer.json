{
    "name": "kirschbaum-development/mail-intercept",
    "description": "A test package for intercepting email sent from Laravel",
    "keywords": [
        "laravel",
        "swift mailer",
        "test",
        "email"
    ],
    "homepage": "https://github.com/kirschbaum-development/mail-intercept",
    "license": "MIT",
    "authors": [
        {
            "name": "Brandon Ferens",
            "email": "brandon@kirschbaumdevelopment.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.0.2",
        "laravel/framework": "^9.2"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^3.2",
        "orchestra/testbench": ">=3.5",
        "phpunit/phpunit": ">=6.0",
        "spatie/ray": "^1.34",
        "swiftmailer/swiftmailer": "^6.0"
    },
    "autoload": {
        "psr-4": {
            "KirschbaumDevelopment\\MailIntercept\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
